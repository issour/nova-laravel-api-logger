{
    "name": "lupka/nova-laravel-api-logger",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "lupka/laravel-api-logger": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Lupka\\NovaLaravelApiLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lupka\\NovaLaravelApiLogger\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
